<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.2.0</Version>
    <Authors>Bernd Ruecker</Authors>
    <Company />
    <Description>A small sample client library around the Camunda 7.x REST API to be used within C# applications. This is meant to be an example and serve as a starting point to copy the code - it is unsupported and not meant for production use.</Description>
    <PackageLicenseUrl>https://github.com/dcomartin/Nancy.Gzip/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/berndruecker/camunda-csharp-client/</PackageProjectUrl>
    <PackageTags>bpmn camunda workflow bpm</PackageTags>
    <Copyright>Copyright 2018 Bernd Ruecker</Copyright>
    <AssemblyVersion>0.0.1.0</AssemblyVersion>
    <FileVersion>0.0.1.0</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
  </ItemGroup>
</Project>